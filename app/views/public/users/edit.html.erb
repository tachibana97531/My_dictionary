<h1>プロフィール編集</h1>
<%= form_with model:@user,url:user_path(@user.id),method: :patch do |f| %>
  <p>名前</p><%= f.text_field:last_name,placeholder:"田中" %><%= f.text_field:first_name,placeholder:"太郎" %>
  <p>ふりがな</p><%= f.text_field:last_name_kana,placeholder:"タナカ" %><%= f.text_field:first_name_kana,placeholder:"タロウ" %>
  <p>自己紹介</p><%= f.text_area:production,placeholder:"自己紹介文を書いてみましょう(この欄は空白でも問題ありません)。" %>
  <p>メールアドレス</p><%= f.text_field:email,placeholder:"tanaka@tarou.com" %>
  <p>電話番号(ハイフンなし)</p><%= f.text_field:telephone_number,placeholder:"00000000000" %>
  <p>郵便番号(ハイフンなし)</p><%= f.text_field:postal_code,placeholder:"0000000" %>
  <p>住所</p><%= f.text_field:address,placeholder:"東京都渋谷区神山町〇-〇" %>
  <%= f.submit "変更を保存",class:"btn btn-sm btn-success" %>
<% end %>